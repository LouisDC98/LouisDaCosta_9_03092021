{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown", "better-docs/component"],
    "templates": {
      "better-docs": {
        "logo": "../src/img/logo.svg"
            }
    },
    "opts": {
      "template": "node_modules/better-docs",
      "destination": "docs",
      "recurse": true,
      "readme": "README.md"
    }
  }